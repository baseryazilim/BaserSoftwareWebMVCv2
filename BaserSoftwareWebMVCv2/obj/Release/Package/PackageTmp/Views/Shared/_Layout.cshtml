<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html" ; charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <title>@ViewBag.Title</title>

    <link href="~/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/css/slider.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />

    <script type="text/javascript" src="~/js/jquery-1.9.0.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.nivo.slider.js"></script>
    <script type="text/javascript">
    </script>
    <script type="text/javascript">
    </script>

    <!-- Projeler Sayfası için Gereken Bileşenler Başlangıcı -->
    <!-- CHANGE TITLE AND META ACCORDINGLY -->
    <!--<title>Apk Manager</title>-->
    <!--<meta name="description" content="Android için modern, kolay ve özelleştirilebilir uygulama yöneticisi">-->
    <meta name="keywords" content="baser, software, android, application, apk, manager">
    <meta name="copyright" content="Copyright © 2018 Baser Software. All Rights Reserved.">
    <meta name="author" content="Baser Software">
    <meta name="theme-color" content="#ffffff">
    <meta content="width=device-width, minimum-scale=1.0, initial-scale=1.0, user-scalable=yes" name="viewport">
    <link rel="stylesheet" href="~/css/tidy.css">
    <link rel="stylesheet" href="~/css/animate.css">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/latest/TweenMax.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/latest/plugins/ScrollToPlugin.min.js"></script>
    <script src="~/bower_components/webcomponentsjs/webcomponents.js"></script>
    <link href="//cdn.rawgit.com/noelboss/featherlight/1.3.2/release/featherlight.min.css" type="text/css" rel="stylesheet" title="Featherlight Styles" />
    <script src="//cdn.rawgit.com/noelboss/featherlight/1.3.2/release/featherlight.min.js" type="text/javascript" charset="utf-8"></script>
    <link rel="import" href="~/bower_components/paper-ripple/paper-ripple.html">
    <link rel="import" href="~/bower_components/paper-fab/paper-fab.html">
    <link rel="import" href="~/bower_components/iron-icons/iron-icons.html">
    <!-- Projeler Sayfası için Gereken Bileşenler Sonu -->

    @Styles.Render("~/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    <div class="header">
        <div class="header_top">
            <div class="wrap">
                <div class="logo">
                    <a href="~/Home/Index"><img src="~/images/logo.png" alt="" /></a>
                </div>
                <div class="menu">
                    <ul>
                        <!--<li><a href="~/Home/Index">Ana Sayfa</a></li>
    <li><a href="~/Home/About">Hakkımızda</a></li>
    <li><a href="~/Projects/Index">Projelerimiz</a></li>-->
                        <li>@Html.ActionLink(layout_res.layout.Ana_Sayfa, "Index", "Home")</li>
                        <li>@Html.ActionLink(layout_res.layout.Hakkimizda, "About", "Home")</li>
                        <li>@Html.ActionLink(layout_res.layout.politika_sayfasi, "Policy", "Home")</li>
                        <li>@Html.ActionLink(layout_res.layout.Projelerimiz, "Projects", "Home")</li>
                        <li>@Html.ActionLink(layout_res.layout.discord_sunucularımız,"Discord","Home")</li>

                    </ul>

                </div>
                <div class="clear"></div>
            </div>
        </div>
    </div>
    @RenderBody()
    <div class="footer">
        <div class="wrap">
            <div class="section group">
                <div class="col_1_of_4 span_1_of_4">
                    <div class="location">
                        <h3>@layout_res.layout.Adres</h3>
                        <ul>
                            <li><img src="~/images/footer_logo.png" alt="" /></li>
                            <li>Barış Mahallesi</li>
                            <li>Barışmanço Bulvarı</li>
                            <li>Barış Apartman No:37/8</li>
                            <li>Kepez/Antalya</li>
                        </ul>
                    </div>
                </div>
                <div class="col_1_of_4 span_1_of_4">
                    <h3>@layout_res.layout.Calisma_Saatlerimiz</h3>
                    <ul>
                        <li>@layout_res.layout.Pazartesi : 13.00-22.00</li>
                        <li>@layout_res.layout.Sali : 13.00-22.00</li>
                        <li>@layout_res.layout.Carsamba : 13.00-22.00</li>
                        <li>@layout_res.layout.Persembe : 13.00-22.00</li>
                        <li>@layout_res.layout.Cuma : 13.00-22.00</li>
                        <li>@layout_res.layout.Cumartesi : 13.00-20.00</li>
                    </ul>
                </div>
                <div class="col_1_of_4 span_1_of_4">
                    <h3>@layout_res.layout.Sosyal_Hesaplar</h3>
                    <div class="Testimonials_desc">
                        <p><li><a href="https://www.facebook.com/baseryazilim/?ref=page_internal">Facebook</a></li></p>
                        <p><li><a href="https://www.instagram.com/baseryazilim/">Instagram</a></li></p>
                    </div>
                </div>
                <div class="col_1_of_4 span_1_of_4">
                    <h3>@layout_res.layout.Iletisim</h3>
                    <ul class="address">
                        <li>baser_yazilim_destek@hotmail.com</li>
                        <li>baseryazilim.destek@gmail.com</li>
                        <li><span>@layout_res.layout.Telefon </span>+90 531 971 6574</li>

                        <li><a href="https://api.whatsapp.com/send?phone=905319716574">@layout_res.layout.Whatsappdan_Mesaj_Gonder</a></li>
                    </ul>

                </div>

            </div>
        </div>
        <div class="copy-right">
            <div class="wrap">
                <p>&copy; @DateTime.Now.Year Başer Software</p><img src="~/images/Norton Secured Logo.png" width="120px" height="64px" alt="" />
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
